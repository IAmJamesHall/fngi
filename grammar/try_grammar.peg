number = whitespace? numeric+ whitespace?

numeric = ~"[0-9]"    # note: ~"..." is a regex
alpha  = ~"[A-Z]"i    # note: i means "ignore case"
alphanumeric = ~"[0-9A-Z]"i
rawStr = ~'"[^"]"'s               # "this is a raw string"
escapedStr = ~'(?s)\\\".*?\\\"'s  # \"this is an escaped string\"

whitespace = " " / "\n"
